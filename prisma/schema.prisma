// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

DATABASE_URL="postgresql://<user>:<password>@localhost:5432/<dbname>?schema=public"

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
